				//- 			use(xlink:href="/images/homeicon.svg#homeicon")
			div.project-see-more
				button.btn.call-to-action See More?


		<!-- Contact Me Section --> 
		.contact-me-section#contact-me-section.fade-in
			div.contact-header
				h1 Reach out to me!
			div.contact-body
				<p> My inbox is always open. Let me know if you have any questions. Iâ€™ll do my best to get back to you! </p>
			button.btn.call-to-action#myBtn2 Say Hello!


		<!-- Footer Section --> 
		div.footer
			div.personal-links
				a.project-icon(href="https://www.linkedin.com/in/gregory-whitman/" target="_blank")
					<svg xmlns="http://www.w3.org/2000/svg" class="footer-link" role="img" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-linkedin">
						<title>LinkedIn</title>
